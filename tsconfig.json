      "outDir": "./dist/",
      "noImplicitAny": true,
      "allowSyntheticDefaultImports": true,
      "module": "esnext",
      "target": "es5",
      "sourceMap": true,
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "lib": ["es6", "dom", "dom.iterable", "esnext"]
    },
    "include": ["src", ".storybook", "declaration.d.ts"],
